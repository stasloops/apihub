{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "next/core-web-vitals"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "useTabs": true,
        "semi": true,
        "trailingComma": "all",
        "bracketSpacing": true,
        "printWidth": 150,
        "endOfLine": "auto"
      }
    ]
  }
}